{
  "id": "openai-service-env",
  "name": "OpenAI Service Environment",
  "values": [
    {
      "key": "baseUrl",
      "value": "http://localhost:5000/api/ai",
      "description": "Base URL for OpenAI service through API Gateway",
      "enabled": true
    },
    {
      "key": "authToken",
      "value": "your-jwt-token-here",
      "description": "Authentication token for protected endpoints",
      "enabled": true
    },
    {
      "key": "timeout",
      "value": "40000",
      "description": "Request timeout in milliseconds",
      "enabled": true
    }
  ],
  "_postman_variable_scope": "environment"
}
